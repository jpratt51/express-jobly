{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "title",
        "salary",
        "equity",
        "companyHandle"
    ],
    "properties": {
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "Consulting civil engineer"
            ]
        },
        "salary": {
            "type": "integer",
            "default": 0,
            "title": "The salary Schema",
            "examples": [
                60000
            ]
        },
        "equity": {
            "type": "integer",
            "default": 0,
            "title": "The equity Schema",
            "examples": [
                0
            ]
        },
        "companyHandle": {
            "type": "string",
            "default": "",
            "title": "The companyHandle Schema",
            "examples": [
                "sellers-bryant"
            ]
        }
    },
    "examples": [{
        "title": "Consulting civil engineer",
        "salary": 60000,
        "equity": 0,
        "companyHandle": "sellers-bryant"
    }]
}